import os
import process_risk as pr

Import('*')

for experiment, config in experiments.iteritems():

    if config['vuln_source'][0] == 'tessler2015':
        env.Command(
                source=[config['vuln_source'][1],
                        config['delta_zeros']],
                target=config['vuln'],
                action=pr.import_idi_vuln)

    env.Command(
            source=config['surge_populations'],
            target=config['surge_1yr_EV_exposure'],
            action=pr.surge_expected_expo)
