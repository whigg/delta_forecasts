import os
import process_srtm as ps

Import('*')

for delta in deltas:
    env.Command(
            source='#data/deltas_shp/deltas.shp',
            target='#data/srtm/{}.tif'.format(delta),
            action=ps.delta_srtm_composite,
            delta=delta)
                

